<body >

    <div class="loading" style="display:none;">Loading&#8230;</div>
    <div class="wrapper">
        <% include partials/sidebar %>
        
        <div class="main-panel " id="main">
            <nav class="navbar navbar-transparent navbar-absolute">
                <div class="container-fluid">
